    "dotenv": "^17.2.3",
    "hono": "^4.11.4",
    "js-yaml": "^4.1.1",
    "node-vibrant": "^4.0.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.8",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
