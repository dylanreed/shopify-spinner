{
  "name": "shopify-spinner",
  "version": "1.0.0",
  "description": "CLI tool to spin up Shopify stores from YAML configs",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "spinner": "./dist/index.js"
  },
  "scripts": {
